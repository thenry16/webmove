<body>

  <div class="topnav card-block">
      <div>
          <a id="navigationBarTitle" >Find a Safe Path</a>
      </div>
  </div>


  <div class="navBar card-block">
      <a href="/index" id="navigationBar hover" style="font-size: 180%; position: relative; top: 7%"><i class="fas fa-home"></i></a>
      <a href="javascript:history.back()" style="font-size: 180%; float: left; padding-top:1%; padding-left: 7%"><i class="fas fa-chevron-left"></i></a>
      <i onclick="dropDown()" class="setting-dropdown fas fa-cog" style="color: white; float: right; padding-right: 10%;"></i>
      <div id="myDropdown" class="dropdown-contents" style="position: relative; left: 60%; height: 15vh; width: 40%">
          <div style="border-bottom: solid black">
                <a href="/FrequentlyAskedQuestions" style="color:black">FAQ</a>
          </div>
          <div>
                <a href="/" style="color:black">Logout</a>
          </div>
      </div>
  </div>



      <div align="center" class="cardboard safetyTips" style="position:relative;top:6vh;" >
          <div class="card-block" style="background-color: #2B7A78; width:80%; border: 1px solid lightgray;border-radius: 4px;" >
              <div class="card-header" style="background-color: #2B7A78;">
                  <h1 style="color: white">Where do you want to go?</h1>
              </div>
          </div>
      </div>

  <div style="position:relative; top:1vh; left:5vw; right:5vw; margin-right:10.3vw;padding-top:50px">
      <div class="cardboard search">
          <form action="/safePath" method="post">
              <div class="form-group centered nice">
                  <img src="images/A_Point.png" >
                  <input required id="origin-input-path" type="text" name="start" value="{{start}}" placeholder="enter a start...">
              </div>
              <div class="form-group centered nice">
                  <img src="images/B_Point.png">
                  <input required id="destination-input-path" type="text" name="destination" value="{{destination}}" placeholder="enter a destination...">
              </div>
              <div class="form-group"style="padding-top:5%;padding-right:6%" align="right">
                  <button class="bouton btn-success" type="submit">Search</button>
              </div>
          </form>
      </div>
  </div>

  {{#if viewAlt}}
      <br><br>
      <div class="col-md-6 main-div mx-auto" style="position: relative; top: 5%">
          <img class="card-img-top centered" src="images/raccoons.jpg" style="width: 100%; margin-left: 28%;">
      </div>
  {{else}}
      <div align="center" class="cardboard safetyTips" style="position:relative;top:15vh;" >
          <div class="card-block"style="background-color: #2B7A78; width:80%;border: 1px solid lightgray;
    border-radius: 4px;">
              <div class="card-header" style="background-color: #2B7A78;" >
                  <h1 style="color:white">Safety tip of the day</h1>
              </div>
          </div>
      </div>
      <div class="safetyTips" style="position:relative; top:15vh;left:5vw">
          <div style='background-color : white; border: 1px solid lightgray;
    border-radius: 4px;'>
              <p>{{tip}}</p>
          </div>
      </div>
  {{/if}}



<script>
    function dropDown() {
        document.getElementById("myDropdown").classList.toggle("show");
    }

    // Close the dropdown menu if the user clicks outside of it
    window.onclick = function(event) {
        if (!event.target.matches('.setting-dropdown')) {

            var dropdowns = document.getElementsByClassName("dropdown-contents");
            var i;
            for (i = 0; i < dropdowns.length; i++) {
                var openDropdown = dropdowns[i];
                if (openDropdown.classList.contains('show')) {
                    openDropdown.classList.remove('show');
                }
            }
        }
    }
</script>
